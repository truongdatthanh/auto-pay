{
  "expo": {
    "name": "AutoPAY",
    "slug": "AutoPAY",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/info.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/info.png",
      "resizeMode": "contain",
      "backgroundColor": "#144c3d"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-autopay.png",
        "backgroundColor": "#144c3d"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.anonymous.AutoPAY"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-barcode-scanner",
      [
        "expo-camera",
        {
          "cameraPermission": "Cho phép AutoPAY mở camera của bạn",
          "cameraPermissionText": "AutoPAY cần quyền truy cập vào camera của bạn để quét mã QR.",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/images/info.png",
          "dark": {
            "image": "./assets/images/info.png",
            "backgroundColor": "#31f123"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow AutoPay to access your Face ID biometric data."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}